      "source": [
        "# Remover 'ID_Cliente' antes da modelagem\n",
        "df = df.drop(['ID_Cliente', 'Churn_Label', 'Churn_Motivo'], axis=1, errors='ignore')"
      ],
      "metadata": {
        "id": "bG9bKf8N-_CK"
